package IBW_SBOL_update.make_doc;

public class Biopart {
	
	public int sequenceLength;
	public String name;
	public String biologicalFunction;
	public String accessionURL;
	public int direction;
	
	Biopart(int length, String partName, String function, String URL, int partDirection){
		//ADD POSITION FIELD. VAlue might be 1,2,3,4
		sequenceLength = length;
		name = partName;
		biologicalFunction = function;
		accessionURL = URL;
		direction = partDirection;
	}
}
